<% include ./../common/header %>
<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <h4 class="card-title">Edit Profile</h4>
                    </div>
                    <div class="card-body">
                        <form>
                            <!-- Status Id -->
                            <div class="row">
                                <div class="col-md-12 pr-1">
                                    <div class="form-group">
                                        <label>Booking ID</label>
                                        <input type="text" name="status_id" class="form-control" placeholder="Status ID"
                                            value="" />
                                    </div>
                                </div>
                            </div>

                            <!-- user id -->
                            <div class="row">
                                <div class="col-md-12 pr-1">
                                    <div class="form-group">
                                        <label>User ID</label>
                                        <input type="text" name="user_id" class="form-control" placeholder="User Id"
                                            value="" />
                                    </div>
                                </div>
                            </div>

                            <!-- start date -->
                            <div class="row">
                                <div class="col-md-12 pr-1">
                                    <div class="form-group">
                                        <label>Start Date</label>
                                        <input type="text" name="start_date" class="form-control"
                                            placeholder="Start Date" value="" />
                                    </div>
                                </div>
                            </div>

                            <!-- end date -->
                            <div class="row">
                                <div class="col-md-12 pr-1">
                                    <div class="form-group">
                                        <label>End Date</label>
                                        <input type="text" name="end_date" class="form-control" placeholder="End Date"
                                            value="" />
                                    </div>
                                </div>
                            </div>

                            <!-- Current Status -->
                            <div class="row">
                                <div class="col-md-12 pr-1">
                                    <div class="form-group">
                                        <label>Current Status </label>
                                        <input type="text" name="current_status" class="form-control"
                                            placeholder="Current Status" value="" />
                                    </div>
                                </div>
                            </div>
                            <!-- equipment ID -->
                            <div class="row">
                                <div class="col-md-12 pr-1">
                                    <div class="form-group">
                                        <label>Equipment ID</label>
                                        <input type="text" name="equipment_id" class="form-control"
                                            placeholder="Equipment ID" value="" />
                                    </div>
                                </div>
                            </div>



                            <button type="submit" class="btn btn-info btn-fill pull-right">
                                Update Profile
                            </button>
                            <div class="clearfix"></div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<% include ./../common/footer %>